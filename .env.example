# Gemini API
GEMINI_API_KEY=your-gemini-api-key-here

# Supabase (shared by backend and frontend)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_PUBLISHABLE_KEY=your-supabase-publishable-key-here
SUPABASE_SECRET_KEY=your-supabase-secret-key-here

# Legacy fallback (to be removed)
SUPABASE_ANON_KEY=your-supabase-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key-here
SUPABASE_JWT_SECRET=your-supabase-jwt-secret-here

# App environment: "dev" (DEBUG logging) or "prod" (INFO logging)
ENV=dev
# Log output format: "text" or "json"
LOG_FORMAT=text
ENABLE_INTERNAL_SCHEDULER=true
CORS_ORIGINS=http://localhost:5173
PIPELINE_TRIGGER_TOKEN=replace-with-random-token

# Development
# VITE_ENABLE_MSW=false  # Uncomment to disable MSW mock API in dev mode
# VITE_API_BASE_URL=http://localhost:8000/api
